{
  "resourceType": "StructureDefinition",
  "id": "CommonOrganization",
  "text": {
    "div": "<div xmlns=\"http://www.w3.org/1999/xhtml\">NZ base Organization profile.</div>",
    "status": "additional"
  },
  "url": "http://hl7.org.nz/fhir/hpi/StructureDefinition/CommonOrganization",
  "version": "0.2.0",
  "name": "CommonOrganization",
  "title": "NZ Organization",
  "status": "active",
  "description": "base NZ Organization profile. Slices on identifier for HPI and uses nzAddress",
  "fhirVersion": "4.0.1",
  "mapping": [
    {
      "identity": "v2",
      "uri": "http://hl7.org/v2",
      "name": "HL7 v2 Mapping"
    },
    {
      "identity": "rim",
      "uri": "http://hl7.org/v3",
      "name": "RIM Mapping"
    },
    {
      "identity": "servd",
      "uri": "http://www.omg.org/spec/ServD/1.0/",
      "name": "ServD"
    },
    {
      "identity": "w5",
      "uri": "http://hl7.org/fhir/fivews",
      "name": "FiveWs Pattern Mapping"
    }
  ],
  "kind": "resource",
  "abstract": false,
  "type": "Organization",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Organization",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Organization.extension",
        "path": "Organization.extension",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "url"
            }
          ],
          "ordered": false,
          "rules": "open"
        }
      },
      {
        "id": "Organization.extension:hl-edi-number",
        "path": "Organization.extension",
        "sliceName": "hl-edi-number",
        "short": "The healthLink EDI address associated with this Organization",
        "min": 0,
        "max": "1"
      },
      {
        "id": "Organization.extension:hl-edi-number.url",
        "path": "Organization.extension.url",
        "fixedUri": "hl-edi-number"
      },
      {
        "id": "Organization.identifier",
        "path": "Organization.identifier",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "use"
            }
          ],
          "rules": "openAtEnd"
        }
      },
      {
        "id": "Organization.identifier:orgId",
        "path": "Organization.identifier",
        "sliceName": "orgId",
        "short": "The current HPI organization Id for this Organization",
        "min": 0,
        "max": "*",
        "mustSupport": true
      },
      {
        "id": "Organization.identifier:orgId.use",
        "path": "Organization.identifier.use",
        "min": 1,
        "patternCode": "official"
      },
      {
        "id": "Organization.identifier:orgId.system",
        "path": "Organization.identifier.system",
        "patternUri": "https://standards.digital.health.nz/id/hpi-org"
      },
      {
        "id": "Organization.address",
        "path": "Organization.address",
        "type": [
          {
            "code": "Address",
            "profile": [
              "http://hl7.org.nz/fhir/StructureDefinition/CommonAddress"
            ]
          }
        ]
      }
    ]
  }
}
